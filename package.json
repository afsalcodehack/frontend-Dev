{
  "name": "picllary-ionic",
  "version": "0.0.1",
  "author": "VIPERdev",
  "description": "Picllary",
  "homepage": "https://viperdev.io/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "build:prod": "ionic-app-scripts build --prod",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "lint": "ionic-app-scripts lint",
    "tslint": "tslint --project . --config ./tslint.json",
    "tsfix": "pnpm run tslint -- --fix",
    "e2e": "pnpm run e2e-update && pnpm run e2e-test",
    "e2e-test": "protractor ./tests/e2e/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false",
    "extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _ --format-indentation='  '"
  },
  "dependencies": {
    "@angular/common": "5.0.1",
    "@angular/compiler": "5.0.1",
    "@angular/compiler-cli": "5.0.1",
