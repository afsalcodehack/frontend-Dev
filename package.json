{
  "name": "picshare-ionic",
  "version": "0.0.1",
  "author": "ViperDev",
  "description": "Picshare",
  "homepage": "https://viperdev.io/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "lint": "ionic-app-scripts lint",
    "tslint": "tslint --project . --config ./tslint.json",
    "tsfix": "npm run tslint -- --fix",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./tests/e2e/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false",
    "extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _ --format-indentation='  '"
  },
  "dependencies": {
    "@angular/common": "5.0.1",
    "@angular/compiler": "5.0.1",
    "@angular/compiler-cli": "5.0.1",
    "@angular/core": "5.0.1",
    "@angular/forms": "5.0.1",
    "@angular/http": "5.0.1",
    "@angular/platform-browser": "5.0.1",
    "@angular/platform-browser-dynamic": "5.0.1",
    "@auth0/angular-jwt": "^1.2.0",
    "@ionic-native/camera": "^4.11.0",
    "@ionic-native/core": "^4.7.0",
    "@ionic-native/file": "^4.11.0",
    "@ionic-native/file-path": "^4.11.0",
    "@ionic-native/globalization": "^4.7.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic-native/transfer": "^3.14.0",
    "@ionic/app-scripts": "^3.1.8",
    "@ionic/storage": "^2.1.3",
    "cordova-android": "7.0.0",
    "cordova-browser": "5.0.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-file-transfer": "^1.7.1",
    "cordova-plugin-filepath": "^1.4.2",
    "cordova-plugin-globalization": "^1.11.0",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "ionic-angular": "3.9.2",
    "ionic-cache": "^2.1.0",
    "ionic-img-viewer": "git+https://github.com/blazeu/ionic-img-viewer.git#image-content",
    "ionic-native": "^2.9.0",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "ng2-translate": "^5.0.0",
    "ngx-stripe": "^0.8.2",
    "rndstr": "^1.0.0",
    "rxjs": "5.5.2",
    "typescript": "2.6.2",
    "url": "0.11.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "^2.8.8",
    "codelyzer": "^4.0.1",
    "jasmine-spec-reporter": "^4.2.1",
    "ng-packagr": "^3.0.6",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.9.1",
    "tslint-eslint-rules": "^5.4.0"
  },
  "engines": {
    "npm": "~5.8.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-globalization": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-camera": {},
      "cordova-plugin-file": {},
      "cordova-plugin-file-transfer": {}
    },
    "platforms": [
      "browser",
      "ios",
      "android"
    ]
  },
  "config": {
    "ionic_enable_lint": false,
    "ionic_copy": "./config/copy.config.js"
  }
}
