    "moment": "^2.22.2",
    "ng2-translate": "^5.0.0",
    "ngx-moment": "^3.1.0",
    "ngx-stripe": "^0.8.2",
    "rxjs": "5.5.2",
    "typescript": "2.6.2",
    "url": "0.11.0",
    "webpack": "3.12.0",
    "zone.js": "0.8.18"
